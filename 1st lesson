#include <iostream>

int main() {
    // Declare two integer variables
    int num1 = 5;
    int num2 = 10;
    
    // Add the two variables
    int sum = num1 + num2;
    
    // Display the variables and the sum
    std::cout << "First number: " << num1 << std::endl;
    std::cout << "Second number: " << num2 << std::endl;
    std::cout << "Sum: " << sum << std::endl;
    
    return 0;
}